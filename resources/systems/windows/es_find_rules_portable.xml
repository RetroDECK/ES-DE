            <entry>%ESPATH%\RetroArch-Win64\retroarch.exe</entry>
            <entry>%ESPATH%\RetroArch\retroarch.exe</entry>
            <entry>%ESPATH%\..\RetroArch-Win64\retroarch.exe</entry>
            <entry>%ESPATH%\..\RetroArch\retroarch.exe</entry>
        </rule>
    </emulator>
    <core name="RETROARCH">
        <rule type="corepath">
            <entry>%EMUPATH%\cores</entry>
        </rule>
    </core>
